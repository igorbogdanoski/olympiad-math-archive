settings.outformat = "pdf";
size(200);
defaultpen(linewidth(1pt));

// Дефинирање на точки
pair A = (0, 3);
pair B = (4, 3);
pair C = (0, 0);
pair D = (4, 0);

// Точки за зиг-заг линијата
pair P1 = (1, 3);     // Точка на горната права
pair P2 = (2.5, 1.5); // Точка на кршење
pair P3 = (1.5, 0);   // Точка на долната права

// Цртање на паралелните прави
draw(A--B, black, Arrow);
draw(C--D, black, Arrow);
label("$a$", B, E);
label("$b$", D, E);

// Цртање на трансверзалите (Зиг-Заг)
draw(P1--P2--P3, blue+1.2);

// Означување на аглите
// Агол горе (40 степени)
draw(arc(P1, 0.4, 270, 315), red);
label("$40^\circ$", P1 + (0.6, -0.3));

// Агол долу (35 степени)
draw(arc(P3, 0.4, 45, 110), red);
label("$35^\circ$", P3 + (0.3, 0.5));

// Непознатиот агол x
draw(arc(P2, 0.4, 135, 230), red);
label("$x$", P2 + (-0.6, 0));

// Додавање точки (опционално за естетика)
dot(P1);
dot(P2);
dot(P3);